#
# Settings for the image load
#

PIXELDB_FILES=${IMAGELOADDATADIR}/J143778stubs.txt
PIX_FULLSIZE=${IMAGELOADDATADIR}/Pix_Fullsize.txt
PIX_THUMBNAIL=${IMAGELOADDATADIR}/Pix_Thumbnail.txt
PIX_MISSING=${IMAGELOADDATADIR}/Pix_MissingFiles.txt
IMAGE_FULLSIZE=${IMAGELOADDATADIR}/image_Fullsize.txt
IMAGEPANE_FULLSIZE=${IMAGELOADDATADIR}/imagepane_Fullsize.txt
IMAGE_THUMBNAIL=${IMAGELOADDATADIR}/image_Thumbnail.txt
IMAGEPANE_THUMBNAIL=${IMAGELOADDATADIR}/imagepane_Thumbnail.txt
COPYRIGHTFILE=${IMAGELOADDATADIR}/IMG_Copyright.in
CAPTIONFILE=${IMAGELOADDATADIR}/IMG_Caption.in
COPYRIGHT_NOTETYPE="Copyright"
CAPTION_NOTETYPE="Caption"
QUALIFIER_FULLSIZE=Fullsize
QUALIFIER_THUMBNAIL=Thumbnail
IMAGE_OBJECTTYPE=Image
IMAGELOADMODE=load
NOTELOADMODE=incremental
IMAGE_ACCESSION=${IMAGELOADDATADIR}/ACC_Accession_Image.bcp
export PIXELDB_FILES PIX_FULLSIZE PIX_THUMBNAIL PIX_MISSING
export IMAGE_FULLSIZE IMAGEPANE_FULLSIZE IMAGE_THUMBNAIL IMAGEPANE_THUMBNAIL
export COPYRIGHTFILE CAPTIONFILE
export COPYRIGHT_NOTETYPE CAPTION_NOTETYPE
export QUALIFIER_FULLSIZE QUALIFIER_THUMBNAIL
export IMAGE_OBJECTTYPE IMAGELOADMODE NOTELOADMODE
export IMAGE_ACCESSION

#
# Settings for the assay load
#

PROBEPREP_FILE=${ASSAYLOADDATADIR}/RT_PCR_probeprep.txt
ASSAY_FILE=${ASSAYLOADDATADIR}/RT_PCR_assay.txt
LANE_FILE=${ASSAYLOADDATADIR}/RT_PCR_gellane.txt
BAND_FILE=${ASSAYLOADDATADIR}/RT_PCR_gelband.txt
ASSAYLOADMODE=load
#ASSAYLOADMODE=preview
export PROBEPREP_FILE ASSAY_FILE LANE_FILE BAND_FILE
export ASSAYLOADMODE

#
# Settings for the index load
#
INDEXLOADMODE=load
IDXPRIORITY=High
IDXCOMMENTS=""
export INDEXLOADMODE IDXPRIORITY IDXCOMMENTS

#
# Miscellaneous settings
#
REFERENCE=J:143778
CREATEDBY=cms
export REFERENCE CREATEDBY
