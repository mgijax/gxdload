#
LOADFILE1=${PROJECTDIR}/SuraniTableS7.txt
LOADFILE2=${PROJECTDIR}/SuraniTableS6Bands.txt
LOADFILE3=${PROJECTDIR}/SuraniTableS6Lanes.txt
LOADFILE5=${PROJECTDIR}/IndexNotes.txt
