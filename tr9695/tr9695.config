EUREXPRESS_DIR=/mgi/all/GXD/Eurexpress-images
FULLSIZE_IMAGE_DIR=${EUREXPRESS_DIR}/Fullsize
THUMBNAIL_IMAGE_DIR=${EUREXPRESS_DIR}/Thumbnails
export EUREXPRESS_DIR FULLSIZE_IMAGE_DIR THUMBNAIL_IMAGE_DIR

#
# Load files and lookups
#
LOADFILE1=${PROJECTDIR}/LoadFile1.txt
LOADFILE2=${PROJECTDIR}/LoadFile2.txt
